<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>FirstServlet</servlet-name>
		<servlet-class>webapp1.FirstServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SecondServlet</servlet-name>
		<servlet-class>webapp1.SecondServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/primeiro</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/primeiro/alternativo</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SecondServlet</servlet-name>
		<url-pattern>/segundo</url-pattern>
	</servlet-mapping>

</web-app>
