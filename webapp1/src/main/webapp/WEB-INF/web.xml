<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>FirstServlet</servlet-name>
		<servlet-class>webapp1.servlet.FirstServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SecondServlet</servlet-name>
		<servlet-class>webapp1.servlet.SecondServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ThirdServlet</servlet-name>
		<servlet-class>webapp1.servlet.ThirdServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/ola</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/primeiro/alternativo</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SecondServlet</servlet-name>
		<url-pattern>/novolivro</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ThirdServlet</servlet-name>
		<url-pattern>/consultalivro</url-pattern>
	</servlet-mapping>

</web-app>
